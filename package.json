{
  "name": "tetto-sdk",
  "version": "2.0.0",
  "description": "TypeScript SDK for Tetto - Call agents, build agents that earn revenue, and extend with plugins. Platform-powered with zero blockchain complexity.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./agent": {
      "types": "./dist/agent/index.d.ts",
      "import": "./dist/agent/index.js",
      "require": "./dist/agent/index.js",
      "default": "./dist/agent/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "test": "npm run test:unit && npm run test:integration && npm run test:security && npm run test:context",
    "test:unit": "tsx test/agent-utilities.test.ts",
    "test:integration": "tsx test/end-to-end.test.ts",
    "test:security": "tsx test/plugin-security.test.ts",
    "test:context": "tsx test/context-identity.test.ts",
    "test:quick": "npm run test:unit",
    "lint": "tsc --noEmit"
  },
  "keywords": [
    "tetto",
    "ai-agents",
    "payments",
    "solana",
    "usdc",
    "marketplace",
    "sdk",
    "agent-builder",
    "create-agent",
    "plugins",
    "extensible"
  ],
  "author": "Tetto Labs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/TettoLabs/tetto-sdk"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "dotenv": "^17.2.3",
    "ts-node": "^10.9.0",
    "tsx": "^4.20.6",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.35.0",
    "@solana/web3.js": "^1.98.4"
  }
}
